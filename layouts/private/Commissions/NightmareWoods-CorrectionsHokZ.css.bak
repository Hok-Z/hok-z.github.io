	/*======== PANEL GŁOWNY KOLOR sprawdz te kolory tla #0000 lub #00000b00 ======== */
	.col-md-9 {
	    background: 0000;
	    min-height: 800px;
	    padding-top: 25px;
	}
	
	
	/* ======== Cień Dookoła prawego panel ========*/ 
	.col-md-3 .panel {
	    border: 1px solid #b3121200;
	    background: #ebeff000;
	    box-shadow: 0px 0px 3px #ddd0;
	}
	
	
	/*======== NaVBar KOLOR - Home, czyj Den i autor CSS z ID ======== */
	.breadcrumb {
	    background: #ffffff1a;
	    font-size: 13px;
		display: block;
	}
	
	/* ======== KOLOR TŁA CAVE GRID ======== */
	.cave-grid {
	    background: #00000061;
	    border-radius: 5px;
	    max-width: 260px;
	    overflow: hidden;
	    padding: 0 0 5px 0;
	    margin: 0 auto;
	    margin-bottom: 15px;
	}
	
	/* ======== KOLOR TŁA MOUND GRID ======== */
	.mound-grid {
	    background: #00000061;
	    border-radius: 10px;
	    width: 100%;
	    overflow: hidden;
	    padding: 5px 5px 10px;
	    margin: 0 auto;
	    margin-bottom: 15px;
	}
	
	/* ======== KOLOR RAMKI ILE ZMUTOWANYCH LWOW a TUTAJ KOLOR OBRAMOWANIA RAMEK NA DOLE======== */
	.table .bottom, .table .top, .table th {
	    text-align: center;
	    background: #000;
		padding: 8px 15px;
	    font-size: 15px;
	    font-weight: bold;
		text-align: center;
	}
	/* ======== KOLOR USTAWIEN KONTA i paskow View Breeding,UserLog,Attack History ========*/
	div .left {
	    background-color: rgba(129, 72, 72, 0) !important;
	    padding: 5px 8px;
	    border-radius: 4px;
	    border: 0;
	}
	
	/* ======== KOLOR TŁA TABELEK profil lwa, prawa strona i zaookrąglenie ======== */
	.inner-table .right {
	    padding: 7px 7px;
	    text-align: left;
	    border-top-right-radius: 1px;
	    border-bottom-right-radius: 1px;
	    background: 000000;
	}
	
	
	/* ======== KOLOR TŁA TABELEK prof lwa, lewa strona ======== */
	.inner-table .left {
	    padding: 7px 7px;
	    text-align: left;
	    border-top-left-radius: 1px;
	    border-bottom-left-radius: 1px;
	    margin-bottom: 2px;
		background: transparent !important;	
	}
	
	
	/* ======== KOLOR TŁA za zdj lwa,zdj gracza, klanami, znajomymi i obrazkami na dole strony (King Dynasty, Pride Dynasty, Achiev, Showcase, Beetle Book) ======== */
  .dynastycollection .left, .table {
	    border: 1px solid #ffffff59 !important;
	    background-color: rgba(0, 0, 0, .3) !important;
	    box-shadow: 0 0 5px #ffffff61 !important;
	}
	
	/* ======== PRIBA ZMIANY KOLORU KLANY I ALLTSOW ======== */
	#about,#sparring, #reclaim, #allies, #clans {
	    border: 1px solid #ffffff59 !important;
	    background-color: rgb (0, 0, 0, .4) !important;
	    box-shadow: 0 0 5px #0000 !important;
	}
	
	#about .left, #sparring .left, #reclaim .left, #allies .left, #clans .left {
	    background: #8ca5a100;
	    border-radius: 0;
	}
	
	/*======== KURWA nie wiem gdzie to belonga ale od dzisiaj tutaj - Kolor boxa naookolo SPARRING ========*/
	.table .right_odd {
	    background: #00000000;
	}
	
	/* ======== KOLOR TŁA GUZIKÓW ======== */
	.select-control, 
	a.button, 
	button.button, 
	input[type="button"], 
	input[type="submit"], 
	input[type="reset"] {
	    background: linear-gradient(to bottom, #8e878700 5%, #2d007e73 5%, #5c73cf 95%) !important;
	    text-shadow: 1px 1px 0px #000;
	    font-weight: bold;
	    font-size: 13px;
	    padding: 3px 12px;
	    margin: 2px 0;
	    border-radius: 3px;
	    color: #fff !important; /* na wszelki wypadek */
	}
	
	
	/* ======== UKRYWANY PANEL ALLIAS - po najechaniu pokazuja sie znajomi ======== */
	#allies tr:not(:first-of-type) {
	  opacity: 0;
	  transition: max-height 0.4s ease, opacity 0.4s ease;
	}
	
	/* ======== UKRYWANY PANEL ALLIAS - po najechaniu pokazuja sie znajomi ======== */
	#allies:hover tr:not(:first-of-type) {
	  opacity: 1;
	}
	
	/* ======== ROZMIAR BOXÓW i CZCIONKA - KingDynasty/PrieDynasty/Achievements/Showcase/BeetleBook ======== */ 
	.dynastylist {
	  width: 160px;       /* mniejsza szerokość, zamiast domyślnej */
	  font-size: 3px;    /* zmniejsza tekst, jeśli chcesz */
	  padding: 0px !important;
		margin: 5px;
	  
	}
	
	
	/* ======== ONLINE MEMBER - Kropka zamieniona na obrazek (tutaj akurat GIF) wyrównany z Nickiem ======== */
	.member-online {
	  background-image: url('https://i.imgur.com/Po40jo6.gif');
	  background-size: contain;
	  background-repeat: no-repeat;
	  background-position: center;
	  width: 90px;
	  height: 65px;
	  display: inline-block;
	  vertical-align: middle;  /* wyrównanie z tekstem */
	  margin-left: 6px;        /* odstęp od nicka */
	  border-radius: 0;
	  background-color: transparent;
	}
	
	
	/* ======== OFFLINE STATUS MEMBER - Kropka zamieniona na szary kolor ======== *
	.member-offline {
	    height: 17px;
	    width: 17px;
	    display: inline-block;
	    background: #3f4042e0
	}
	
	/* ======== Ustawia PANEL FeedAll/PlayAll pod sobą, bez ingerencji w dynastylist - ALSO Event Banners / Snuhlions / Pumpkins======== */
	.feature {
	  width: 100% !important;
	  display: block;
	  overflow: hidden;
	  transition: max-height 0.5s ease;
	  background-color: rgba(255, 255, 255, 0.05);
	  border: 1px solid #999;
	  border-radius: 6px;
	  color: white !important;
	  padding: 10px;
	  margin-bottom: 12px;
	  position: relative;
	  box-sizing: border-box;
	}
	
	/* ======== PANEL FeedAll/PlayAll Styl nagłówka ======== */
	.feature h3 {
	  font-size: 21px;
	  margin: 0 0 5px 0;
	}
	
	
	/* ======== IMIE LWA/KRÓLA [ ]'s DEN NAGŁOWEK ROZMIAR CZCIONKI WYŚRODKOWANY ======== */
	h1 {
	  text-align: center;              /* Wyśrodkowanie poziome */
	  font-size: 32px;                 /* Rozmiar czcionki */
	  font-weight: bold;              /* Pogrubienie tekstu */
	  font-family: 'Bitter', serif;  /* Czcionka */
	  margin-top: 50px;               /* Górny margines */
	  letter-spacing: 7px;            /* Rozstaw liter */
	}
	
	/* ======== ZAOKRĄGLENIE DO RAMEK DO WSZAYSTKIEGO OPRÓCZ Alertów, Paska Home gdzie jest kreator CSS, napisów na dole strony View Breeding User Log Attack History ======== */
	table, td, th, .table-responsive, .feature {
	  border-radius:7px !important;
	
	}
	/* ======== ZAOKRĄGLENIE DO RAMEK ale nie wiem o co chozi bo tylko trzyma zaookraglenie i kolor naokolo Clan Memberships jak usuniesz to przestaje dzialac ramka wtf ======== */
	table {
	  border-collapse: separate !important;
	  border-spacing: 0 !important;
	
	}
	

	
	 .progress, .topbar, .navbar, .cave-grid, .mound-grid, .feature, .dynastycollection div, .main, .panel, #editdetailspanel, #pageContainer > center > form > table > tbody > tr > th {
	  animation: pulse-border 3.5s ease-in-out infinite !important;
	}
	

	#about.table tbody, #player tbody, #sparring tbody, #allies tbody, #clans tbody, #attack {
	  animation: pulse-border 3.5s ease-in-out infinite;
	  border-radius: 10px;
	}

/* ======= RAMKA PULSUJĄCA KOLOROWA DOOKOŁA wszystkiego oprócz napisów View Breedinng,Userlog,Attack History, Account Settings ======= */
	@keyframes pulse-border {
	  0%, 100% {
	    box-shadow: 0 0 10px #ffffffc7;
	  }
	  50% {
	    box-shadow: 0 0 10px #3d3d3f;
	  }
	}
	
	.auto tbody {
		box-shadow: none !important;
		animation: none !important;
	}
	
	
	/*Kolor CHATBOXA kolor wiadomości na przemian*/

	.s-chat-message:nth-child(2n) {
	background-color: #696c7063 !important;
	}
	
	
	.s-chat-message:nth-child(2n+1) {
	background-color: rgba(107, 154, 217, .32) !important;
	}
	
	
	#btnLoadNewMessages, #btnChatSettings, .emoteInput , .s-chat-message_menu {
	    background-color: transparent !important;
	    border: none !important;
	}
	
	
	/* ======== UKRYTY BOX  "Social Media" ======== */ 
	#pageSidebar > div.col-md-12.col-xs-12 > div:nth-child(4) {
	    display: none !important;
	}
	input.floatright {
	  visibility: hidden !important;
	  pointer-events: none !important;
	}

/* Feed All Play All Hover*/
#pageContainer > center > div:nth-child(32) > form:nth-child(1) > div > div,
#pageContainer > center > div:nth-child(32) > form:nth-child(2) > div > div {
	height: 30px;
	transition: 1s;
}

#pageContainer > center > div:nth-child(32) > form:nth-child(1) > div > div:hover,
#pageContainer > center > div:nth-child(32) > form:nth-child(2) > div > div:hover {
	height: 150px !important;
	transition: 1s;
}

#pageContainer > center > div:nth-child(43) > div:nth-child(1) > a > div,
#pageContainer > center > div:nth-child(43) > div:nth-child(2) > a > div,
#pageContainer > center > div:nth-child(43) > div:nth-child(3) > a > div,
#pageContainer > center > form:nth-child(46) > div.left {
		background: black !important;
	    animation: pulse-border 3.5s ease-in-out infinite;
	    border: 1px solid #ffffff59 !important;
	    box-shadow: 0 0 5px #ffffff61 !important;
		border-radius: 15px;
	}
	