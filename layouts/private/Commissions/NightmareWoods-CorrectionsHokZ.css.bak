	/* ======== GOOGLE FONT BITTER ======== */
	@import url('https://fonts.googleapis.com/css2?family=Bitter:ital,wght@0,100..900;1,100..900&display=swap');
	
	
	/* ========OGÓLNA CZCIONKA: BITTER + BIAŁY KOLOR ======== */
	.container, .content, .box, td, th, a, p, div, span, h1, h2, h3, h4, h5, h6, li {
	    font-family: 'Bitter', serif !important;
	    color: #c4c4c4 !important;
	}
	
	
	/* ======== GŁOWNE TŁO PROFILU ======== */
	body {
	    background-image: url('https://i.imgur.com/eYwpfhk.jpeg') !important;
	    background-size: cover !important;
	    background-repeat: no-repeat !important;
	    background-attachment: fixed !important;
	    background-position: center top !important;
	    font-family: 'Bitter', serif !important;
	    color: #c4c4c4 !important;
	}
	
	/* ======== Topbar & Navbar ======== */
	.topbar  {
	  height: 35px;
	  border-radius: 0px 0px 7px 7px !important
	}
	
	.main {
	  border-radius: 7px !important;
	}
	
	
	.navbar {
	  height: 35px;
	  z-index: 2;
	  border-radius: 7px 7px 0px 0px !important;
	}
	
	/* ======== Color on HOVER "Den/Crosssroads/Hoard/Oasis/Chatter/News/Event/Faq" ======== */
	.navbar-default .navbar-nav li a:hover {
	        background: #3b3b3c;
	}
	
	/* ======== RAMKA DOOKOŁA CAŁEGO PROFILU ==== usnięte .bottom czyli kreska nad napisem ile mutacji jest w pridzie ======== */
	.main, .topbar, .navbar, .panel, .feature,
	.comment, .lionImage {
	    background-color: rgba(0, 0, 0, .57) !important;
	    color: #e0e0e000 !important;
	    border: 1px solid #ffffff59 !important;
	    box-shadow: 0 0 5px #ffffff59;
		
	}
	
	@media (max-width: 767px) {
	#bs-example-navbar-collapse-1 {
	    background-image: url('https://i.imgur.com/Yso7Lwp.jpeg');
	}
	
	#bs-example-navbar-collapse-1 a {
		color: white !important;
		background: #0000008a;
		border-radius: 10px;
	}
	
	#bs-example-navbar-collapse-1 a:hover {
		background: #000000c7;
	}
	}
	
	
	/*======== RAMKA DOOKOŁA "spar" i Edit Detail KOLOR W SORDKU RAMKI ======== */
